{
  "name": "@shaneholloman/creator",
  "version": "0.1.0",
  "description": "Project scaffolding for TypeScript applications and libraries",
  "author": "Shane Holloman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shaneholloman/creator"
  },
  "bin": {
    "creator": "dist/cli.js"
  },
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check": "biome check --write src/ && tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "istextorbinary": "^9.5.0",
    "js-yaml": "^4.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/js-yaml": "^4.0.9",
    "@types/prompts": "^2.4.9",
    "husky": "^9.1.7",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "cli",
    "docker",
    "caddy",
    "deployment",
    "templates"
  ]
}
